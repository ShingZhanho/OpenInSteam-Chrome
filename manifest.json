{
    "name": "Open In Steam",
    "description": "Opens all steam pages in your steam client.",
    "version": "0.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts":[{
        "js": [
            "script.js"
        ],
        "css": [
            "page-element-style.css"
        ],
        "matches":[
            "*://steamcommunity.com/*"
        ],
        "run_at": "document_end"
    }
    ]
}